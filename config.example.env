# Copy this file to .dev.vars for local development
# Never commit .dev.vars to version control

# Microsoft OAuth Configuration
MICROSOFT_CLIENT_ID=your_microsoft_client_id_here
MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret_here
MICROSOFT_TENANT_ID=your_tenant_id_here_optional

# PandaDoc Integration
PANDADOC_CLIENT_ID=your_pandadoc_client_id_here
PANDADOC_CLIENT_SECRET=your_pandadoc_client_secret_here

# HubSpot Integration
HUBSPOT_CLIENT_ID=your_hubspot_client_id_here
HUBSPOT_CLIENT_SECRET=your_hubspot_client_secret_here

# Xero Integration (optional - only needed if enabled)
XERO_CLIENT_ID=your_xero_client_id_here
XERO_CLIENT_SECRET=your_xero_client_secret_here

# NetSuite Integration (optional - only needed if enabled)
NETSUITE_CLIENT_ID=your_netsuite_client_id_here
NETSUITE_CLIENT_SECRET=your_netsuite_client_secret_here

# Autotask Integration (optional - only needed if enabled)
AUTOTASK_CLIENT_ID=your_autotask_client_id_here
AUTOTASK_CLIENT_SECRET=your_autotask_client_secret_here

# Session Management
COOKIE_ENCRYPTION_KEY=your_32_char_encryption_key_here_min

# Optional: Error Tracking & Observability
SENTRY_DSN=your_sentry_dsn_here_optional
SENTRY_SAMPLE_RATE=0.1
ENVIRONMENT=development
SENTRY_RELEASE=your_release_version_optional
SENTRY_ENABLE_LOGS=false 